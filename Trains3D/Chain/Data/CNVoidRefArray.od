package core.chain

import core.chain.CNMemory._

stub struct CNVoidRefArray(length : uint, bytes : VoidRef)
{
    static def apply(length : uint) : CNVoidRefArray = CNVoidRefArray(length, malloc(length))
    static def apply(tp: ODPType<_>, count : uint) : CNVoidRefArray = CNVoidRefArray(tp.size * count)

    def write(tp : any, item : any) : CNVoidRefArray

    def write(uInt4 : uint4) :CNVoidRefArray

    def free {
        free(bytes)
    }
}